{
  "name": "react-falcor",
  "version": "0.7.2",
  "description": "React binding for Falcor",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "del lib",
    "prepublish": "run-s clean build",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx,md .",
    "size": "size-limit",
    "test": "run-p test:* lint",
    "test:jest": "jest",
    "test:size": "run-s build size",
    "precommit": "lint-staged"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "eventemitter3": "^2.0.3",
    "hoist-non-react-statics": "^2.3.1",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.6.0",
    "recompose": "^0.26.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.10",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.1.0",
    "del-cli": "^1.1.0",
    "delay": "^2.0.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-concise": "^0.14.0",
    "eslint-config-concise-esnext": "^0.14.0",
    "eslint-config-concise-react": "^0.13.0",
    "falcor": "^2.0.0-rc1",
    "falcor-json-graph": "^2.0.0",
    "falcor-router": "^0.8.1",
    "husky": "^0.14.3",
    "jest": "^21.2.1",
    "lint-staged": "^4.3.0",
    "npm-run-all": "^4.1.1",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "react-test-renderer": "^16.0.0",
    "size-limit": "^0.11.6"
  },
  "browserslist": [
    "last 2 versions",
    "IE >= 9"
  ],
  "repository": "ratson/react-falcor",
  "license": "MIT",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "keywords": [
    "falcor",
    "react"
  ],
  "jest": {
    "setupFiles": [
      "<rootDir>/node_modules/raf/polyfill.js",
      "<rootDir>/test/helpers/enzyme-config.js"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "13 KB"
    }
  ]
}
