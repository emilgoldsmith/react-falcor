{
  "name": "react-falcor",
  "version": "0.7.2",
  "description": "React binding for Falcor",
  "main": "lib/index.js",
  "scripts": {
    "build": "cross-env NODE_ENV=production babel --source-maps --out-dir=lib/ src/",
    "dev": "cross-env NODE_ENV=development babel --watch --source-maps --out-dir=lib/ src/",
    "clean": "del lib",
    "prepublish": "run-s clean build",
    "lint": "eslint --ignore-path .gitignore --ext js,jsx,md .",
    "size": "size-limit",
    "test": "run-p test:* lint",
    "test:jest": "jest",
    "test:size": "run-s build size"
  },
  "peerDependencies": {
    "react": "*"
  },
  "dependencies": {
    "babel-runtime": "^6.26.0",
    "eventemitter3": "^2.0.3",
    "hoist-non-react-statics": "^2.3.1",
    "invariant": "^2.2.2",
    "lodash": "^4.17.4",
    "prop-types": "^15.5.10",
    "recompose": "^0.25.0",
    "warning": "^3.0.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-eslint": "^8.0.0",
    "babel-jest": "^21.0.2",
    "babel-plugin-dev-expression": "^0.2.1",
    "babel-plugin-lodash": "^3.2.11",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.8",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.5",
    "del-cli": "^1.1.0",
    "delay": "^2.0.0",
    "enzyme": "^2.9.1",
    "eslint": "^4.7.2",
    "eslint-config-concise": "^0.12.0",
    "eslint-config-concise-esnext": "^0.12.0",
    "eslint-config-concise-react": "^0.12.0",
    "falcor": "^2.0.0-rc1",
    "falcor-json-graph": "^1.1.7",
    "falcor-router": "^0.8.1",
    "jest": "^21.1.0",
    "npm-run-all": "^4.1.1",
    "react": "^15.6.1",
    "react-addons-test-utils": "^15.6.0",
    "react-dom": "^15.6.1",
    "react-test-renderer": "^15.6.1",
    "size-limit": "^0.11.4"
  },
  "browserslist": [
    "last 2 versions",
    "IE >= 9"
  ],
  "repository": "ratson/react-falcor",
  "license": "MIT",
  "files": [
    "*.md",
    "LICENSE",
    "lib",
    "src"
  ],
  "keywords": [
    "falcor",
    "react"
  ],
  "size-limit": [
    {
      "path": "lib/index.js",
      "limit": "20 KB"
    }
  ]
}
